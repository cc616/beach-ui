{
  "name": "beach-ui",
  "version": "0.0.9",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "clean": "rimraf dist lib es",
    "build:css": "cd build && gulp",
    "build:common": "cd build && rollup --config rollup.config.common.js",
    "build:umd": "cd build && rollup --config rollup.config.js",
    "build": "yarn clean && yarn build:common && yarn build:umd && yarn build:css"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "LICENSE"
  ],
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "beach",
    "beach-ui",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "framework",
    "frontend"
  ],
  "tags": [
    "beach",
    "beach-ui",
    "react",
    "react-ui",
    "component",
    "components",
    "javascript",
    "es6",
    "css3"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "8.2.3",
    "classnames": "2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "5.2.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^5.0.0",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "2.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "4.6.2"
  }
}
